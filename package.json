{
  "name": "@braisenly/mui",
  "version": "0.0.2",
  "packageManager": "pnpm@9",
  "repository": {
    "type": "git",
    "url": "https://github.com/zautke/braisenly-mui"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "*.css",
    "./augments.ts",
    "./types.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "dist",
    "colors.d.ts",
    "mui5.d.ts"
  ],
  "type": "module",
  "scripts": {
    "test": "vitest",
    "build": "tsup index.ts --format cjs,esm --dts",
    "lint": "tsc --noEmit",
    "changeset": "changeset",
    "version:bump": "changeset version",
    "commit:release": "bash scripts/commit-release.sh",
    "tag:version": "node -p \"require('./package.json').version\" | xargs -I % git tag v%",
    "tag:push": "node -p \"require('./package.json').version\" | xargs -I % git push origin v%",
    "publish:public": "bash scripts/publish-public.sh",
    "release": "pnpm run changeset && pnpm run version:bump && pnpm run commit:release && pnpm run tag:version && pnpm run tag:push && pnpm run publish:public"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^6.0.0",
    "@mui/material": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "@mui/icons-material": {
      "optional": true
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/victor-mono": "^5.0.0",
    "@mui/icons-material": "^6.0.0",
    "@mui/material": "^6.0.0",
    "@types/lodash": "^4.17.23",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "csstype": "^3.2.3",
    "jsdom": "^22.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vitest": "^0.34.0"
  }
}
